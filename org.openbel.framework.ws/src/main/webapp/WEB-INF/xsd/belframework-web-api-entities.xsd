    <xs:simpleType name="KAMLoadStatus">
        <xs:annotation>
            <xs:documentation>
                A set of statuses indicating the load state of a KAM.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="IN_PROCESS">
                <xs:annotation>
                    <xs:documentation>
                        Indicates a KAM that is being loaded.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="COMPLETE">
                <xs:annotation>
                    <xs:documentation>
                        Indicates a KAM that has been loaded.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="FAILED">
                <xs:annotation>
                    <xs:documentation>
                        Indicates a KAM that has failed to load.
                    </xs:documentation>
                </xs:annotation>
